interface Animal {
  species: String!
  habitat: Habitat!
}

interface Habitat {
  description: String
  temperature: Float
}

type Sea implements Habitat {
  description: String
}

type Desert implements Habitat {
  description: String
}

interface Pet {
  name: String!
}

type Cat implements Animal & Pet {
  species: String!
  habitat: Habitat!
  name: String!
  name1: String!
  name2: String!
}

type Turtle implements Animal & Pet {
  species: String!
  habitat: Habitat!
  name: String!
  shell: String!
}

type Lion implements Animal {
  habitat: Habitat!
  species: String!
  name: String!
}

type Query {
  friend: Animal
  random: Any
  number: Int!
  double: Float!
}

union Any = Cat | Lion | Turtle | Sea | Desert